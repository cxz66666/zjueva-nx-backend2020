@page
@model _2020_backend.Pages.Records.ExportModel
@{
    ViewData["Title"] = "导出";
}

<h1>导出数据</h1>

<form class="m-2" method="post">
    <div class="form-group mx-sm-0 mb-2">
        <input asp-for="PasserOnly" /> 只导出通过面试的人 <br />
    </div>
    <div class="form-group mx-sm-0 mb-2">
        <input asp-for="Lastest" />只导出最新的报名数据<br />
    </div>
    <div class="form-group mx-sm-0 mb-2">
        <input asp-for="IsJson" />导出为json<br />
    </div>
    <button type="submit" class="btn btn-outline-primary mb-2">GO!</button>
</form>